platform :ios, ‘9.0’

    inhibit_all_warnings!               #忽略全部警告

target ‘KACat’ do
pod 'AFNetworking', '~> 3.1.0'          #网络请求框架
pod 'SDWebImage', '~> 4.3.3'            #加载网络图片，实现缓存效果
pod 'MBProgressHUD', '~> 1.0.0'         #hud效果
pod 'MJRefresh'                         #快速集成下拉刷新、上拉更多
pod 'FMDB', '~> 2.7.2'               	#代码布局管理


end
